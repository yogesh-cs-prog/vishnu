            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarCollapse">
            <div class="navbar-nav ms-auto p-4 p-lg-0">
                <a href="/" class="nav-item nav-link active">Home</a>
                <!-- <a href="about.html" class="nav-item nav-link">About</a> -->
                <a href="#" class="nav-item nav-link">Services</a>
                
                <a href="#" class="nav-item nav-link">Contact</a>
               

           

                
                <% if(user) { %>
                    <div class="nav-item dropdown">
                        <a href="/auth/userLogin" class="nav-item nav-link">  <i class="fa-solid fa-user">&ensp;</i>  <%= user.userName %></a>
                        <div class="dropdown-menu fade-up m-0 user-div">
                           
                                <form action="/totalBookings" method="post">
                                    <% if(user) { %>
                                        <!-- <input> -->
                                         <input type="hidden" value="<%= user._id %>" name="userId">
                                        <button class="user-btn dropdown-item" type="submit">Bookings</button>
                                        
                                    <% } %>
                                </form>
                            
                            <form action="/logout" method="get">
                            
                               
                                    <% if(user) { %>
                                        <button class="dropdown-item user-btn" type="submit">Logout</button>
                                        
                                    <% } %>
                                </form>
                            
                           
                        </div>
                    </div>
                    

                <% } else { %>
                    <a href="/auth/userLogin" class="nav-item nav-link">Login</a>
                    <% } %>
            <a href="/" class="btn btn-primary py-4 px-lg-5 d-none d-lg-block">Get A Quote<i class="fa fa-arrow-right ms-3"></i></a>
