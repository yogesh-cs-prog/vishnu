    <style>
       

       .container-1 {
            max-width: auto;
            display: inline-block;
            margin: 15px;
            padding: 20px;
            background-color: #d6cdcd;
            box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
            border-radius: 10px;
        }

        .container-1 h1 {
            margin-bottom: 15px;
            color: #D81324;
            font-size: 24px;
            font-weight: bold;
        }

        .container-1 p {
            color: #333;
            font-size: 16px;
            margin-bottom: 10px;
        }

        .container-1 p strong {
            color: #007bff;
        }

        .container-1 hr {
            border-top: 1px solid #007bff;
            margin: 20px 0;
        } 
        .btn-custom{
            border: none;
            background-color: white !important;
            text-decoration: none;
        }
        .user-btn{
            background-color: white !important;
            border: none;
        }
        .user-btn:hover{
            background-color: white !important;
            border: none;
        }
        .user-div a{
            background-color: white !important;
        }
        .user-div a:hover{
            background-color: #d81324 !important;
        }

        
    </style>
  

