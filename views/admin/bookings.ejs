  <link rel="stylesheet" href="../../assets/css/style.css">
    <style>
        body{
            overflow-x: hidden;
        }
        .min-vw-50 {
            min-width: 25vw !important;

        }

        .mb-10 {
            margin-bottom: 10vh !important;
        }
    </style>
</head>
<body class="y-bg-color">
    <aside class="sidebar y-bg-color">
        <button type="button" class="sidebar-close-btn">
          <iconify-icon icon="radix-icons:cross-2"></iconify-icon>
        </button>
               <div>
          <a href="/" class="sidebar-logo min-w-100%">
            <img src="../../assets/images/logo-name.jpg" alt="site logo" class="light-logo ">
            <img src="../../assets/images/logo-name.jpg" alt="site logo" class="dark-logo ">
            <!-- <img src="../../assets/images/logo-icon.png" alt="site logo" class="logo-icon "> -->
          </a>
        </div>
        <div class="sidebar-menu-area">
          <ul class="sidebar-menu" id="sidebar-menu">
            <li>
              <a href="/api/admin">
                <iconify-icon icon="solar:home-smile-angle-outline" class="menu-icon"></iconify-icon>
                <span>Dashboard</span>
              </a>
            </li>
            <li>
              <a href="/api/admin/addService">
                <iconify-icon icon="ion:search-outline" class="menu-icon"></iconify-icon>
                <span>Services</span>
              </a>
            </li>
            <li>
              <a href="/api/admin/bookings">
                <iconify-icon icon="hugeicons:invoice-03" class="menu-icon"></iconify-icon>
                <span>Bookings</span> 
              </a>
            </li>
            <!-- <li>
              <a href="/user">
                <iconify-icon icon="flowbite:users-group-outline" class="menu-icon"></iconify-icon>
                <span>Users</span>
              </a>
            </li>
              <li>
                <a href="/logout">
                  <iconify-icon icon="solar:logout-2-outline" class="menu-icon"></iconify-icon>
                  <span>logout</span>
                </a>
              </li> -->
          </ul>
        </div>
      </aside>



    <main class="dashboard-main y-bg-color">
        <div class="navbar-header y-bg-color">
            <div class="row align-items-center justify-content-between">
                <div class="col-auto">
                    <div class="d-flex flex-wrap align-items-center gap-4">
                        <button type="button" class="sidebar-toggle">
                            <iconify-icon icon="heroicons:bars-3-solid" class="icon text-2xl non-active"></iconify-icon>
                            <iconify-icon icon="iconoir:arrow-right" class="icon text-2xl active"></iconify-icon>
                        </button>
                        <button type="button" class="sidebar-mobile-toggle">
                            <iconify-icon icon="heroicons:bars-3-solid" class="icon"></iconify-icon>
                        </button>
                        <form class="navbar-search " action="/search-db" method="post">
                            <input type="text" name="Phone" placeholder="Search" class="y-bg-color">
                            <iconify-icon icon="ion:search-outline" class="icon"></iconify-icon>
                        </form>
                    </div>
                </div>
                <div class="col-auto">
                    <div class="d-flex flex-wrap align-items-center gap-3">
                        <button type="button" data-theme-toggle
                            class="w-40-px h-40-px bg-neutral-200 rounded-circle d-flex justify-content-center align-items-center"></button>

                        <div class="dropdown">
                            <button class="d-flex justify-content-center align-items-center rounded-circle"
                                type="button" data-bs-toggle="dropdown">
                                <img src="assets/images/user.png" alt="image"
                                    class="w-40-px h-40-px object-fit-cover rounded-circle">
                            </button>
                            <div class="dropdown-menu to-top dropdown-menu-sm">
                                <div
                                    class="py-12 px-16 radius-8 bg-primary-50 mb-16 d-flex align-items-center justify-content-between gap-2">
                                    <div>

                                        <h6 class="text-lg text-primary-light fw-semibold mb-2">Welcome Admin,</h6>
                                        <span class="text-secondary-light fw-medium text-sm">Admin</span>
                                    </div>
                                    <button type="button" class="hover-text-danger">
                                        <iconify-icon icon="radix-icons:cross-1" class="icon text-xl"></iconify-icon>
                                    </button>
                                </div>
                                <ul class="to-top-list">
                                    <li>
                                        <a class="dropdown-item text-black px-0 py-8 hover-bg-transparent hover-text-danger d-flex align-items-center gap-3"
                                            href="/logout">
                                            <iconify-icon icon="lucide:power" class="icon text-xl"></iconify-icon>
                                            Log Out</a>
                                    </li>
                                </ul>
                            </div>
                        </div><!-- Profile dropdown end -->


                    </div>
                </div>
            </div>
        </div>
        </div>
        </div>
        </div>
        </div>

        <div class="dashboard-main-body">

            <div class="d-flex flex-wrap align-items-center justify-content-between gap-3 mb-24">
                <h6 class="fw-semibold mb-0">Bookings</h6>

                <ul class="d-flex align-items-center gap-2">
                    <form action="/api/admin/filterBookings" method="post" style="display: flex;flex-direction: row;align-items: center;gap: 20px;">
                        <select class="form-select form-select-sm w-auto ps-10 py-6 radius-12 h-45-px" name="filter">
                          <option disabled selected>Status</option>
                          <option value="pending">pending</option>
                          <option value="completed">completed</option>
                      </select>
                       
                      <button type="submit" style="color: red; background-color: aliceblue; padding: 6px 15px 6px 15px;border: 1px solid gray;">Filter</button>
                     
                      </form>
                    <li class="fw-medium">
                        <a href="/" class="d-flex align-items-center gap-1 hover-text-primary">
                            <iconify-icon icon="solar:home-smile-angle-outline" class="icon text-lg"></iconify-icon>
                            Dashboard
                        </a>
                    </li>
                    <li>-</li>
                    <li class="fw-medium">Users</li>
                </ul>
            </div>


          <h6><%= total %> Bookings Found</h6>  

            <% Details.forEach((detail)=> { %>
                <div class="d-inline-flex gap-1 mb-10" style="display: flex;">
                    <form action="/api/admin/updateStatus" method="post">

                        <input type="hidden" value="<%= detail._id %>" name="booking_id">
                        <a class="col-xxl-4 col-sm-10 mob-tab icon-mob min-vw-50">
                            <button class="row-xxl-4 col-sm-10 icon-mob" type="submit">
                                <div class="card h-100 radius-12 bg-gradient-success text-center icon-mob">
                                    <div class="card-body p-24 icon-mob">
                                        <div
                                            class="w-64-px h-64-px d-inline-flex align-items-center justify-content-center bg-success-600 text-white mb-16 radius-12 icon-mob">
                                            <iconify-icon icon="mingcute:user-add-fill" class="h5 mb-0"></iconify-icon>
                                        </div>
    
                                        <div>
                                            <div style="text-align: start;">
    
                                                <p><strong>User Name:</strong>
                                                    <%= detail.userName %>
                                                </p>
                                                <p><strong>Reg. No :</strong>
                                                    <%= detail.regno %>
                                                </p>
                                                <p><strong>Bike Brand:</strong>
                                                    <%= detail.brand %>
                                                </p>
                                                <p><strong>Bike Model:</strong>
                                                    <%= detail.model %>
                                                </p>
                                                <p><strong>Phone Number:</strong>
                                                    <%= detail.userPhoneNumber %>
                                                </p>
                                                <p><strong>Status: </strong>
                                                    <%= detail.status %>
                                                </p>
                                               
    
                                            </div>
                                        </div>
    
    
    
                                    </div>
                                </div>
                            </button>
                        </a>
                    </form>







                </div>
                <% }) %>


                    <script src="script.js"></script>

                    <!-- jQuery library js -->
                    <script src="../../assets/js/lib/jquery-3.7.1.min.js"></script>
                    <!-- Bootstrap js -->
                    <script src="../../assets/js/lib/bootstrap.bundle.min.js"></script>
                    <!-- Apex Chart js -->
                    <script src="../../assets/js/lib/apexcharts.min.js"></script>
                    <!-- Data Table js -->
                    <script src="../../assets/js/lib/dataTables.min.js"></script>
                    <!-- Iconify Font js -->
                    <script src="../../assets/js/lib/iconify-icon.min.js"></script>
                    <!-- jQuery UI js -->
                    <script src="../../assets/js/lib/jquery-ui.min.js"></script>
                    <!-- Vector Map js -->
                    <script src="../../assets/js/lib/jquery-jvectormap-2.0.5.min.js"></script>
                    <script src="../../assets/js/lib/jquery-jvectormap-world-mill-en.js"></script>
                    <!-- Popup js -->
                    <script src="../../assets/js/lib/magnifc-popup.min.js"></script>
                    <!-- Slick Slider js -->
                    <script src="../../assets/js/lib/slick.min.js"></script>
                    <!-- main js -->
                    <script src="../../assets/js/app.js"></script>

                    <script src="../../assets/js/homeFiveChart.js"></script>

</body>

</html>